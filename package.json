{
	"name": "komentar",
	"version": "0.1.0",
	"description": "Just a commenting system",
	"main": "./src/main.ts",
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"watch": "tsc -w",
		"start": "nodemon",
		"prebuild": "rimraf dist && rimraf public && webpack",
		"build": "npm run prebuild && tsc -p tsconfig.build.json",
		"serve": "node dist/main.js",
		"test": "jest --coverage",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
	},
	"author": "Ari Maulana <maulana.ari@protonmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^26.0.0",
		"@types/node": "^14.0.13",
		"nodemon": "^2.0.4",
		"rimraf": "^3.0.2",
		"ts-jest": "^26.1.1",
		"ts-node": "^8.10.2",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^3.9.5"
	},
	"dependencies": {
		"@nestjs/common": "^7.2.0",
		"@nestjs/core": "^7.2.0",
		"@nestjs/platform-express": "^7.2.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^6.5.5",
		"uuid": "^8.2.0"
	}
}
