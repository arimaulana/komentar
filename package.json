{
  "name": "komentar",
  "version": "0.1.0",
  "description": "Just a commenting system",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:fe:dev": "webpack-dev-server ./frontend/index.html",
    "build:fe": "npm run build:fe:clean && npm run build:fe:webpack",
    "build:fe:clean": "rm -rf public && mkdir public",
    "build:fe:webpack": "webpack",
    "test": "jest --coverage",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "author": "Ari Maulana <maulana.ari@protonmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/platform-express": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "uuid": "^8.2.0"
  }
}
